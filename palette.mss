@streetcar-cl: #009fc3;
@streetcar-ns: #8dc63f;
@wes: #000000;
@bus: #7f99b1;
@tm-district: #773300;

//colors from old style
@park-old: #d5ddbb;
@water-old: #c9d9d9;
@water-casing-old: #abc4c4;